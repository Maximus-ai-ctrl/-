CREATE TABLE coffee (
    id INTEGER PRIMARY KEY,
    name TEXT NOT NULL,
    roast_level TEXT NOT NULL,
    type TEXT NOT NULL,
    flavor_description TEXT NOT NULL,
    price REAL NOT NULL,
    packaging_volume TEXT NOT NULL
);

